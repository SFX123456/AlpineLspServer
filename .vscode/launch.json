  "version": "0.2.0",
  "configurations": [
    {
      "type": "extensionHost",
      "request": "launch",
      "name": "Launch Client",
      "runtimeExecutable": "C:\\Users\\Nico\\AppData\\Roaming\\nvm\\v18.17.1\\node.exe",
      "outFiles": ["${workspaceRoot}/client/out/**/*.js"],
      "preLaunchTask": {
        "type": "npm",
        "script": "postinstall"
        
      }
    }
  ]
}
